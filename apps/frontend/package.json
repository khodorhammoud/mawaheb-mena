  "scripts": {
    "build": "pnpm exec remix vite:build",
    "dev": "pnpm exec remix vite:dev --host 0.0.0.0 --port 5173",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "pnpm exec remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "playwright test --debug",
    "test": "pnpm e2e",
    "test:setup-db": "node ../../scripts/setup-test-db.js",
    "test:teardown-db": "node ../../scripts/teardown-test-db.js",
    "test:with-db": "pnpm test:setup-db && pnpm test && pnpm test:teardown-db",
    "db:generate:test": "drizzle-kit generate --config=drizzle.test.config.ts",
    "db:push:test": "drizzle-kit push --config=drizzle.test.config.ts"
  },
