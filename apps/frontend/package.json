{
  "name": "frontend",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev ",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx  app/db/migrator.ts",
    "db:drop": "drizzle-kit drop --out=app/db/drizzle/migrations",
    "db:seed": "tsx app/db/seeders/seeder.ts",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx app tests --ignore-pattern build --ignore-pattern dist --ignore-pattern node_modules",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx app tests --ignore-pattern build --ignore-pattern dist --ignore-pattern node_modules --fix",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:integration:watch": "vitest --config vitest.integration.config.ts",
    "test": "npm run test:unit && npm run test:integration"
  },
