{
  "name": "@mawaheb/db",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "type": "module",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./server": {
      "import": {
        "types": "./dist/server.d.ts",
        "default": "./dist/server.mjs"
      },
      "require": {
        "types": "./dist/server.d.ts",
        "default": "./dist/server.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/server.ts --format esm,cjs --dts ",
    "dev": "tsup src/index.ts src/server.ts --format esm,cjs --watch --dts ",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/migrations/migrator.ts",
    "db:migrate:fresh": "tsx src/migrations/migrate-fresh.ts",
    "db:migrate:fresh:seed": "tsx src/migrations/migrate-fresh.ts --seed",
    "db:seed": "tsx src/seeders/seeder.ts"
  },
  "devDependencies": {
    "drizzle-kit": "^0.30.6",
    "drizzle-dbml-generator": "^0.10.0",
    "drizzle-orm": "^0.41.0",
    "eslint": "^8.56.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "@faker-js/faker": "^9.6.0"
  },
  "dependencies": {
    "bcrypt-ts": "^5.0.0",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "postgres": "^3.4.4"
  },
  "peerDependencies": {
    "drizzle-orm": "^0.41.0"
  }
}
